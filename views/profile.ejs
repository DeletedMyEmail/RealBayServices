<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Profile Page</title>
    <script src="https://kit.fontawesome.com/b2cb25ccdb.js" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa" crossorigin="anonymous" defer></script>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <link href="css/styles.css" rel="stylesheet">
    <link href="css/profile.css" rel="stylesheet">

    <script  src ="js/event-handler.js" type="text/javascript"defer></script>
    
  </head>
  <body id="body">
    <div class="cnavbar">
      <button id="logobtn"  class="navbarLogo" type="button" name="button">
        <img class="navbarLogo" src="images/Logo.png" alt="Realbay-Logo">
      </button>

        <form class="searchForm" action="/" method="post">
            <div class="dropdown-center">
                <button class="cbtn cbtn-tags" type="button" data-bs-toggle="dropdown" aria-expanded="false">Tags</button>
                <ul class="dropdown-menu">
                    <li>
                        <input class="dropdown-item" name="c1-checkbox" type="checkbox">
                        <label for="c1-checkbox"> Dienstleistungen</label>
                    </li>
                    <li> <a class="dropdown-item" href="#"> Dropdown item 2 &raquo; </a>
                      <ul class="submenu dropdown-menu">
                        <li><a class="dropdown-item" href="#">Submenu item 1</a></li>
                        <li><a class="dropdown-item" href="#">Submenu item 2</a></li>
                        <li><a class="dropdown-item" href="#">Submenu item 3 &raquo; </a>
                          <ul class="submenu dropdown-menu">
                            <li><a class="dropdown-item" href="#">Multi level 1</a></li>
                            <li><a class="dropdown-item" href="#">Multi level 2</a></li>
                          </ul>
                        </li>
                        <li><a class="dropdown-item" href="#">Submenu item 4</a></li>
                        <li><a class="dropdown-item" href="#">Submenu item 5</a></li>
                      </ul>
                    </li>
                    <li><a class="dropdown-item" href="#"> Dropdown item 3 </a></li>
                    <li><a class="dropdown-item" href="#"> Dropdown item 4 </a></li>
                  </ul>
            </div>

            <input class="searchbar" type="text" name="searchInput">
            <button class="cbtn" type="submit">Search</button>
        </form>

        <button class="cbtn cbtn-profile"><i class="fa-solid fa-user"></i></button>
    </div>

    <!-- mainsite -->
    <div class=" card text-bg-success mb-5" id="PersonalInfo" >
      <div class="card-header fs-4 ">
        <%=UserName%> <br> ⭐⭐⭐⭐
      </div>
      <div class="card-body">
        <img src="https://images.ctfassets.net/hrltx12pl8hq/5Bn4tHITZdTAQTkuX43hCk/46687c228ddc7325554601dff7d0ebc5/thumb_june22_03.jpg?fit=fill&w=240&h=135&fm=webp" alt="profilbild" class="profile-img">
        <div class="socials">
            <a href="#" target="_blank">
                <i class="fa-solid fa-1x fa-envelope responsive-icon icon"></i>
                <%=Email%>
            </a>
            </a><br>
            <a href="#" target="_blank">
                <i class="fa-solid fa-1x fa-phone responsive-icon icon"></i>
              <%=Tel%>
            </a><br>
            <a href="#" target="_blank">
              <i class="fa-brands fa-1x fa-twitter responsive-icon icon"></i>
              <%=Twitter%>
            </a> <br>
            <a href="#" target="_blank">
              <i class="fa-brands fa-1x fa-linkedin-in responsive-icon icon"></i>
              <%=LinkedIn%>
            </a>
        </div>
      </div>
    </div>

    <div class="bio">
      <h1>Bio</h1>
      <p><%=Bio%></p>

      <div id="carouselExampleControls" class="carousel slide" data-bs-ride="carousel">
